
# Primitive data types convrsion;

import java.util.*;

public class data{
    public static void main(String[] arg){

        int i=10;
        double d= 20.99;
        char c='z';
        boolean b=true;

        System.out.println("Values before conversion: ");

        System.out.println("Integer: "+i);
        System.out.println("Double: "+d);
        System.out.println("Character: "+c);
        System.out.println("Boolean: "+b);

        System.out.println("Values after conversion: ");

        double D=(double)i;
        int C=(int)c;

        System.out.println("Integer: "+D);
        System.out.println("Character: "+C);





    }
}
